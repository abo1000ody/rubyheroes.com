%section.content
  .main
    %table
      %caption
        #{@nominator} (#{@nominator.email})
      %thead
        %tr
          %th(scope="col") Nominee
          %th(scope="col") Testimony
      %tbody
        - @nominator.nominations.each do |nomination|
          %tr
            %td.testimony= link_to nomination.nominee, admin_nominee_path(nomination.nominee)
            %td.testimony= nomination.testimonial

    %p= link_to "Back to nominators", admin_nominators_path
